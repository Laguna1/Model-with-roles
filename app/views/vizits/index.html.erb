<p id="notice"><%= notice %></p>

<h1>Vizits</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Place</th>
      <th>Notes</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vizits.each do |vizit| %>
      <tr>
        <td><%= vizit.date %></td>
        <td><%= vizit.place %></td>
        <td><%= vizit.notes %></td>
        <td><%= vizit.user_id %></td>
        <td><%= link_to 'Show', vizit %></td>
        <td><%= link_to 'Edit', edit_vizit_path(vizit) %></td>
        <td><%= link_to 'Destroy', vizit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vizit', new_vizit_path %>
